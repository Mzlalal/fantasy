<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.mzlalal.oauth2.dao.ClientDao">

    <!-- 可根据自己的需求，是否要使用 -->
    <resultMap type="com.mzlalal.base.entity.oauth2.ClientEntity" id="clientMap">
        <result property="id" column="id"/>
        <result property="clientId" column="client_id"/>
        <result property="clientSecret" column="client_secret"/>
        <result property="responseType" column="response_type"/>
        <result property="redirectUri" column="redirect_uri"/>
        <result property="scope" column="scope"/>
        <result property="resourceId" column="resource_id"/>
        <result property="roleId" column="role_id"/>
        <result property="accessTokenTime" column="access_token_time"/>
        <result property="refreshTokenTime" column="refresh_token_time"/>
        <result property="autoApprove" column="auto_approve"/>
    </resultMap>


</mapper>